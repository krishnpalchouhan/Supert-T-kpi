<?xml version='1.0' encoding='utf-8'?>
<widget id="kpireports.superttransport.com" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OneSignalPush">
        <param name="android-package" value="com.plugin.gcm.OneSignalPush" />
    </feature>
    <allow-intent href="market:*" />
    <name>KPI Reports</name>
    <description>
       Provides employees of Super T Transport Inc. with various KPI reports.
    </description>
    <author email="sales@rockstarwebmarketing.com" href="http://cordova.io">
         Rockstar Web Marketing LLC
    </author>
    <content src="index.html" />
    <icon src="www/img/super_t_app_logo.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
